<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <size android:height="10dp"
                android:width="10dp" />
        </shape>
    </item>
    <item android:height="7dp"
        android:width="7dp"
        android:gravity="center">
        <shape
