<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/bg_white_true_cloudv3_upload_dialog"
    android:orientation="vertical"
    android:paddingBottom="@dimen/true_cloudv3_spacing_l">

    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:paddingHorizontal="@dimen/true_cloudv3_spacing_m"
        android:paddingTop="@dimen/true_cloudv3_spacing_l"
        android:paddingBottom="@dimen/true_cloudv3_spacing_2s">

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/collapseImageView"
            android:layout_width="@dimen/true_cloudv3_spacing_l"
            android:layout_height="@dimen/true_cloudv3_spacing_l"
            android:src="@drawable/ic_baseline_keyboard_arrow_down_24"
            android:tint="@color/black" />

        <androidx.appcompat.widget.AppCompatTextView
            style="@style/TrueID.Header3.Bold.Black"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/true_cloudv3_spacing_2s"
            android:text="@string/true_cloudv3_dialog_option" />

    </androidx.appcompat.widget.LinearLayoutCompat>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginHorizontal="@dimen/true_cloudv3_spacing_m"
        android:background="@color/true_cloudv3_line_separator_color2" />

    <androidx.appcompat.widget.LinearLayoutCompat
        android:id="@+id/trueCloudShareContainer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:paddingHorizontal="@dimen/true_cloudv3_spacing_m"
        android:paddingTop="@dimen/true_cloudv3_spacing_2m"
        android:paddingBottom="@dimen/true_cloudv3_spacing_2s">

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/iconShareImageView"
            android:layout_width="@dimen/true_cloudv3_spacing_l"
            android:layout_height="@dimen/true_cloudv3_spacing_l"
            android:src="@drawable/ic_baseline_ios_share_24" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/trueCloudShareTextView"
            style="@style/TrueID.Header3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/true_cloudv3_spacing_m"
            android:text="@string/true_cloudv3_share" />
    </androidx.appcompat.widget.LinearLayoutCompat>

    <androidx.appcompat.widget.LinearLayoutCompat
        android:id="@+id/trueCloudRenameContainer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:paddingHorizontal="@dimen/true_cloudv3_spacing_m"
        android:paddingTop="@dimen/true_cloudv3_spacing_2m"
        android:paddingBottom="@dimen/true_cloudv3_spacing_2s">

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/iconUploadImageView"
            android:layout_width="@dimen/true_cloudv3_spacing_l"
            android:layout_height="@dimen/true_cloudv3_spacing_l"
            android:src="@drawable/ic_rename_24" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/trueCloudTextViewRename"
            style="@style/TrueID.Header3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/true_cloudv3_spacing_m"
            android:text="@string/true_cloudv3_dialog_rename" />
    </androidx.appcompat.widget.LinearLayoutCompat>

    <androidx.appcompat.widget.LinearLayoutCompat
        android:id="@+id/trueCloudPhotoEditorContainer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:paddingHorizontal="@dimen/true_cloudv3_spacing_m"
        android:paddingTop="@dimen/true_cloudv3_spacing_2m"
        android:paddingBottom="@dimen/true_cloudv3_spacing_2s">

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/iconPhotoEditorImageView"
            android:layout_width="@dimen/true_cloudv3_spacing_l"
            android:layout_height="@dimen/true_cloudv3_spacing_l"
            android:src="@drawable/ic_true_cloudv3_photo_editor" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/trueCloudTextViewPhotoEditor"
            style="@style/TrueID.Header3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/true_cloudv3_spacing_m"
            android:text="@string/true_cloudv3_dialog_photo_editor" />
    </androidx.appcompat.widget.LinearLayoutCompat>

    <androidx.appcompat.widget.LinearLayoutCompat
        android:id="@+id/trueCloudDownLoadContainer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:paddingHorizontal="@dimen/true_cloudv3_spacing_m"
        android:paddingTop="@dimen/true_cloudv3_spacing_2m"
        android:paddingBottom="@dimen/true_cloudv3_spacing_2s">

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/iconRenameImageView"
            android:layout_width="@dimen/true_cloudv3_spacing_l"
            android:layout_height="@dimen/true_cloudv3_spacing_l"
            android:src="@drawable/ic_download_24" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/trueCloudDownloadTextView"
            style="@style/TrueID.Header3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/true_cloudv3_spacing_m"
            android:text="@string/true_cloudv3_dialog_download" />
    </androidx.appcompat.widget.LinearLayoutCompat>

    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:paddingHorizontal="@dimen/true_cloudv3_spacing_m"
        android:paddingTop="@dimen/true_cloudv3_spacing_2m"
        android:paddingBottom="@dimen/true_cloudv3_spacing_2s">

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/iconDeleteImageView"
            android:layout_width="@dimen/true_cloudv3_spacing_l"
            android:layout_height="@dimen/true_cloudv3_spacing_l"
            android:src="@drawable/ic_truecloud_delete_24" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/trueCloudDeleteTextView"
            style="@style/TrueID.Header3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/true_cloudv3_spacing_m"
            android:text="@string/true_cloudv3_dialog_delete" />
    </androidx.appcompat.widget.LinearLayoutCompat>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/trueCloudViewFileLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingHorizontal="@dimen/true_cloudv3_spacing_m"
        android:paddingTop="@dimen/true_cloudv3_spacing_2m"
        android:paddingBottom="@dimen/true_cloudv3_spacing_2s">

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/iconViewFileImageView"
            android:layout_width="@dimen/true_cloudv3_spacing_l"
            android:layout_height="@dimen/true_cloudv3_spacing_l"
            android:src="@drawable/ic_baseline_remove_red_eye_24"
            app:layout_constraintBottom_toBottomOf="@id/trueCloudViewFileTextView"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@id/trueCloudViewFileTextView" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/trueCloudViewFileTextView"
            style="@style/TrueID.Header3"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/true_cloudv3_spacing_m"
            android:layout_marginEnd="@dimen/true_cloudv3_spacing_m"
            android:text="@string/true_cloudv3_dialog_view_file"
            app:layout_constraintEnd_toStartOf="@id/viewFileProgressBar"
            app:layout_constraintStart_toEndOf="@+id/iconViewFileImageView"
            app:layout_constraintTop_toTopOf="parent" />

        <ProgressBar
            android:id="@+id/viewFileProgressBar"
            style="?android:attr/progressBarStyle"
            android:layout_width="@dimen/true_cloudv3_spacing_l"
            android:layout_height="@dimen/true_cloudv3_spacing_l"
            android:layout_centerHorizontal="true"
            android:indeterminateTint="@color/true_cloudv3_color_black"
            android:visibility="gone"
            app:layout_constraintBottom_toBottomOf="@id/trueCloudViewFileTextView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@id/trueCloudViewFileTextView"
            tools:visibility="visible" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.appcompat.widget.LinearLayoutCompat
        android:id="@+id/trueCloudInfoContainer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:paddingHorizontal="@dimen/true_cloudv3_spacing_m"
        android:paddingTop="@dimen/true_cloudv3_spacing_2m"
        android:paddingBottom="@dimen/true_cloudv3_spacing_2s">

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/iconSeeInfoImageView"
            android:layout_width="@dimen/true_cloudv3_spacing_l"
            android:layout_height="@dimen/true_cloudv3_spacing_l"
            android:src="@drawable/ic_baseline_info_24" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/trueCloudSeeInfoTextView"
            style="@style/TrueID.Header3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/true_cloudv3_spacing_m"
            android:text="@string/true_cloudv3_see_file_info" />
    </androidx.appcompat.widget.LinearLayoutCompat>

</androidx.appcompat.widget.LinearLayoutCompat>
